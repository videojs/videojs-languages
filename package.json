{
  "name": "videojs-languages",
  "author": "Brightcove, Inc.",
  "license": "Apache-2.0",
  "version": "1.0.0",
  "main": "src/api.js",
  "bin": {
    "vjslang": "src/cli.js"
  },
  "engine": {
    "node": ">=8"
  },
  "keywords": [
    "videojs",
    "cli",
    "languages",
    "i18n",
    "l10n"
  ],
  "vjsstandard": {
    "ignore": [
      "bin",
      "docs",
      "es5",
      "es5-test"
    ]
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "mocha": "^5.2.0",
    "mock-fs": "^4.7.0",
    "npm-run-all": "~4.1.3",
    "tsmlb": "^1.0.0",
    "videojs-standard": "^8.0.2",
    "conventional-changelog-videojs": "^3.0.0",
    "conventional-changelog-cli": "^2.0.1",
    "doctoc": "^1.3.1",
    "husky": "^1.0.1",
    "lint-staged": "^7.2.2",
    "not-prerelease": "^1.0.1",
    "npm-merge-driver-install": "^1.0.0",
    "videojs-generator-verify": "~1.0.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "scripts": {
    "lint": "vjsstandard",
    "pretest": "vjsstandard",
    "test": "mocha -u tdd test/*.test.js",
    "preversion": "npm test",
    "version": "is-prerelease || npm run update-changelog && git add CHANGELOG.md",
    "update-changelog": "conventional-changelog -p videojs -i CHANGELOG.md -s",
    "prepublishOnly": "vjsverify --skip-es-check"
  },
  "lint-staged": {
    "*.js": [
      "vjsstandard --fix",
      "git add"
    ],
    "README.md": [
      "doctoc",
      "git add"
    ]
  },
  "dependencies": {
    "cli": "^1.0.1",
    "flatten": "1.0.2",
    "globby": "^8.0.1",
    "mkdirp": "^0.5.1"
  }
}
